{
    "version": 2,
    "builds": [
      {
        "src": "next.config.js",
        "use": "@vercel/next"
      },
      {
        "src": "pages/**/*.js",
        "use": "@vercel/next"
      },
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "functions": {
      "api/**/*.js": {
        "maxDuration": 10
      }
    },
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }
  